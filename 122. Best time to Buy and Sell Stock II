class Solution {
    public int maxProfit(int[] prices) {
        int oldprice = prices[0];
        int profit  = 0;
        for(int i = 1; i < prices.length; i++){
            if(prices[i] < oldprice){
                oldprice = prices[i];
            }else{
                profit += prices[i] - oldprice;
                oldprice = prices[i];

            }
        }

        return profit;

    }
}
